<% @title = "Profil" %>

<%= form_for @paper, :html => {:multipart => true} do |f| %>
    <p>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </p>

    <p>
      <% @pdf = "Nur PDF" %>
       <%= f.label @pdf%>
      <%= f.file_field :attachment  %>
    </p>

    <p><%= f.submit %></p>
<% end %>


