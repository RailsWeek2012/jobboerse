<%= form_for @paper, :html => {:multipart => true} do |f| %>
    <%= f.error_messages %>
    <%= f.hidden_field :user_id %>
    <p>
      <%= f.label :name %><br />
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.file_field :file %>
    </p>
    <p>
      <%= f.label :remote_image_url, "or image URL" %><br />
      <%= f.text_field :remote_image_url %>
    </p>
    <p><%= f.submit %></p>
<% end %>

